{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "tsoa": "ts-node-dev tsoaconfig.ts",
    "start": "NODE_OPTIONS='--max-old-space-size=20480' ts-node --transpile-only src/index.ts",
    "dev": "ts-node-dev --inspect=0.0.0.0:9229 --poll --respawn --transpile-only --max-old-space-size=20480 src/index.ts",
    "tsc:check": "tsc --noEmit",
    "migration:generate": "ts-node-dev $(npm bin)/typeorm migration:generate -n sql",
    "migration:run": "ts-node-dev $(npm bin)/typeorm migration:run",
    "migration:revert": "ts-node-dev $(npm bin)/typeorm migration:revert"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-and-time": "^1.0.1",
    "date-fns-timezone": "^0.1.4",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "inversify": "^5.0.5",
    "inversify-binding-decorators": "^4.0.0",
    "luxon": "^1.25.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "tsoa": "^3.4.0",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.12",
    "@types/date-and-time": "^0.13.0",
    "@types/express": "^4.17.9",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.16",
    "@types/uuid": "^8.3.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.1",
    "typescript": "~4.5.5"
  }
}
